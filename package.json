{
  "name": "formal-photos",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev --proxy 3000 -- npm run dev:frontend",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:cli": "bash scripts/run_e2e_cli.sh"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "testMatch": [
      "**/__tests__/**/*.test.{ts,tsx}"
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "face-api.js": "^0.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "jest": "^30.2.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "wrangler": "^4.63.0"
  }
}
